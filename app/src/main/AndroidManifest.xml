<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.softsignproj">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Softsignproj"
        tools:targetApi="31">
        <activity
            android:name=".myEvents.ScheduledEvents"
            android:exported="false" />
        <activity
            android:name=".myEvents.MyEvents"
            android:exported="false" />
        <activity
            android:name=".AdminPage"
            android:exported="false"
            android:parentActivityName=".SignIn" />
        <activity
            android:name=".venueList.VenueList"
            android:exported="false"
            android:parentActivityName=".HomePage" />
        <activity
            android:name=".CreateAccount"
            android:exported="false"
            android:parentActivityName=".SignIn" />
        <activity
            android:name=".SignIn"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomePage"
            android:exported="false"
            android:parentActivityName=".SignIn" />
        <activity
            android:name=".addVenue.AddVenue"
            android:exported="true"
            android:parentActivityName=".AdminPage" />
        <activity
            android:name=".addVenue.AddVenueEnterSports"
            android:exported="true"
            android:parentActivityName=".addVenue.AddVenue" />
        <activity
            android:name=".joinEvent.JoinEventsPage"
            android:exported="true"
            android:parentActivityName=".HomePage" />
        <activity
            android:name=".filterEvents.AdminFilterEventsPage"
            android:exported="true"
            android:parentActivityName=".AdminPage">
        </activity>
        <activity android:name=".scheduleEvent.ScheduleEvent"
            android:exported="true"
            android:parentActivityName=".venueList.VenueList" />
    </application>
</manifest>